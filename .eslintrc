{
    "extends": "@react-native-community",
    "rules": {
        "prettier/prettier": [
            "error",
            {
              "singleQuote": true,
              "parser": "flow"
            }
          ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            2,
            "single",
            {
                "avoidEscape":true
            }
        ],
        "semi": [
            "error",
            "always"
        ]
      }
    
}